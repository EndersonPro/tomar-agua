{
  "name": "Tomar Agua | Drink water",
  "description": "Extensi√≥n que te recuerda tomar agua. | Extension that reminds you to drink water",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["alarms", "notifications", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Tomar Agua | Drink water",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/water_logo_extension16.png",
    "32": "img/water_logo_extension32.png",
    "48": "img/water_logo_extension48.png",
    "128": "img/water_logo_extension128.png",
    "256": "img/water_logo_extension.png"
  }
}
